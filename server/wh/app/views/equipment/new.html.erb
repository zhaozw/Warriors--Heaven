<h1>New equipment</h1>

<% form_for(@equipment) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :eqname %><br />
    <%= f.text_field :eqname %>
  </p>
  <p>
    <%= f.label :prop %><br />
    <%= f.text_field :prop %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', equipment_path %>